{
  "name": "rapid-dotenv",
  "version": "1.0.5",
  "author": "XuanNguyen <xuan.0211@gmail.com>",
  "license": "MIT",
  "description": "Loads environment variables from `.env.[development|test|production][.local]` files",
  "keywords": [
    "dotenv",
    "node_env",
    "development",
    "test",
    "production",
    "local",
    "env",
    "environment",
    "variables"
  ],
  "homepage": "https://github.com/xuanhoa88/rapid-dotenv",
  "repository": {
    "type": "git",
    "url": "https://github.com/xuanhoa88/rapid-dotenv.git"
  },
  "bugs": {
    "url": "https://github.com/xuanhoa88/rapid-dotenv/issues"
  },
  "main": "./dist/dotenvify.js",
  "module": "./dist/dotenvify.js",
  "types": "./dist/dotenvify.d.ts",
  "bin": {
    "dotenvify": "./dist/config.js"
  },
  "exports": {
    ".": "./dist/dotenvify.js",
    "./config": "./dist/config.js"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "conventional-changelog-cli": "2.0.35",
    "eslint": "8.22.0",
    "eslint-config-prettier": "8.10.0",
    "eslint-plugin-prettier": "5.2.1",
    "jest": "28.1.3",
    "prettier": "3.3.3",
    "tmp": "0.2.1",
    "typescript": "4.9.5"
  },
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "prepare": "npm run build",
    "build": "tsc --build",
    "rebuild": "npm run build",
    "test": "jest",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "test:types": "tsc --noEmit",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "lint": "eslint '**/*.{js,ts}'",
    "lint:fix": "npm run lint -- --fix"
  }
}
